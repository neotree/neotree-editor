{
  "name": "neotree-editor",
  "description": "Neo Tree - Content Manager",
  "author": "Matteo Giaccone <m.giaccone@ubiqueworks.com>",
  "license": "ISC",
  "main": "main.js",
  "private": true,
  "bin": {
    "concurrently": "./node_modules/.bin/concurrently",
    "rimraf": "node_modules/.bin/rimraf",
    "babel": "./node_modules/.bin/babel",
    "cross-env": "./node_modules/.bin/cross-env"
  },
  "scripts": {
    "start": "babel-node _server/index.js",
    "clean-dist": "npm run remove-dist && mkdir dist && mkdir dist/src",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node _tools/buildHtml.js",
    "build:server": "babel _server --out-dir dist/_server --copy-files --ignore .gitignore",
    "postbuild:server": "ncp _server/tmp_uploads dist/_server/tmp_uploads",
    "build:config": "babel _config --out-dir dist/_config --copy-files",
    "build:utils": "babel _utils --out-dir dist/_utils --copy-files",
    "prebuild": "npm-run-all clean-dist build:html build:server build:config build:utils",
    "build": "babel-node _tools/build.js",
    "start:prod-server": "cross-env NEOTREE_SERVER_TYPE=production node dist/_server/index.js",
    "start:stage-server": "cross-env NEOTREE_SERVER_TYPE=stage node dist/_server/index.js",
    "start:dev-server": "cross-env NEOTREE_SERVER_TYPE=development node dist/_server/index.js",
    "prod-server": "pm2 --name neotree_webeditor_production --watch --no-daemon start npm -- run start:prod-server",
    "stage-server": "pm2 --name neotree_webeditor_stage --watch --no-daemon start npm -- run start:stage-server",
    "dev-server": "pm2 --name neotree_webeditor_development --watch --no-daemon start npm -- run start:dev-server"
  },
  "build": {
    "appId": "org.neotree.NeoTreeEditor",
    "mac": {
      "category": "public.app-category.medical",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "asar": false,
    "productName": "neotree",
    "draft": true
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^7.0.0",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^5.3.1",
    "firebase-admin": "^8.13.0",
    "install": "^0.12.2",
    "isomorphic-fetch": "^2.2.1",
    "material-design-icons-iconfont": "^5.0.1",
    "multer": "^1.4.2",
    "npm": "^6.14.5",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "randomstring": "^1.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.21",
    "react-icons": "^3.10.0",
    "react-mdc-web": "^0.17.0",
    "react-mdl": "^1.11.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-sortable-hoc": "^1.11.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "roboto-fontface": "^0.10.0",
    "sequelize": "^6.2.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.10.4",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "colors": "^1.4.0",
    "css-loader": "^3.6.0",
    "del": "^4.1.1",
    "electron": "^5.0.13",
    "electron-builder": "^20.44.4",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-packager": "^14.2.1",
    "eslint": "^5.16.0",
    "eslint-config-rallycoding": "^3.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.2",
    "eslint-watch": "^5.1.2",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^4.3.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
