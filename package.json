{
  "name": "neotree-editor",
  "description": "Neo Tree - Content Manager",
  "author": "Matteo Giaccone <m.giaccone@ubiqueworks.com>",
  "license": "ISC",
  "main": "main.js",
  "private": true,
  "bin": {
    "concurrently": "./node_modules/.bin/concurrently",
    "rimraf": "node_modules/.bin/rimraf",
    "babel": "./node_modules/.bin/babel"
  },
  "scripts": {
    "start": "babel-node _server/index.js",
    "clean-dist": "npm run remove-dist && mkdir dist && mkdir dist/src",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node _tools/buildHtml.js",
    "build:server": "babel _server --out-dir dist/_server --copy-files --ignore .gitignore",
    "postbuild:server": "ncp _server/tmp_uploads dist/_server/tmp_uploads",
    "build:config": "babel _config --out-dir dist/_config --copy-files",
    "build:utils": "babel _utils --out-dir dist/_utils --copy-files",
    "prebuild": "npm-run-all clean-dist build:html build:server build:config build:utils",
    "build": "babel-node _tools/build.js",
    "dist": "pm2 start dist/_server/index.js --name neotree --watch --no-daemon"
  },
  "build": {
    "appId": "org.neotree.NeoTreeEditor",
    "mac": {
      "category": "public.app-category.medical",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "asar": false,
    "productName": "neotree",
    "draft": true
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@hot-loader/react-dom": "^16.8.6",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^6.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "express-validator": "^5.3.1",
    "firebase-admin": "^8.5.0",
    "install": "^0.12.2",
    "isomorphic-fetch": "^2.2.1",
    "material-design-icons-iconfont": "^5.0.1",
    "multer": "^1.4.1",
    "npm": "^6.9.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "prop-types": "^15.7.2",
    "randomstring": "^1.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.11.1",
    "react-icons": "^3.7.0",
    "react-mdc-web": "^0.17.0",
    "react-mdl": "^1.11.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-sortable-hoc": "^1.9.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "roboto-fontface": "^0.10.0",
    "sequelize": "^5.8.12",
    "socket.io": "^2.2.0",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "colors": "^1.3.3",
    "css-loader": "^3.0.0",
    "del": "^4.1.1",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-packager": "^14.0.0",
    "eslint": "^5.16.0",
    "eslint-config-rallycoding": "^3.2.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-react": "^7.13.0",
    "eslint-watch": "^5.1.2",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^4.0.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
